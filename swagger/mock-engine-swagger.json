{
  "swagger": "2.0",
  "info": {
    "description": "test mock - engine",
    "version": "1.0",
    "title": "mock server - engine"
  },
  "paths": {
    "/get-info": {
      "get": {
        "description": "get engine mock",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "array",
              "minItems": 30,
              "items": {
                "$ref": "#/definitions/resEngineGetInfo"
              }
            }
          },
          "400": {
            "description": "Invalid Request Parameters or Request Body"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
  },
  "definitions": {
    "numberModel": {
      "type": "integer",
      "minimum": 1,
      "maximum": 5000
    },
    "stringModel": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{5,25}$"
    },
    "engineFuel": {
      "type": "string",
      "enum": [
        "GASOLINE",
        "DIESEL",
        "ELECTRIC"
      ]
    },
    "usageStatus": {
      "type": "string",
      "enum": [
        "USED",
        "READY",
        "PENDING",
        "UNUSABLE"
      ]
    },
    "resEngineGetInfo": {
      "type": "object",
      "properties": {
        "EnginId": {
          "$ref": "#/definitions/numberModel"
        },
        "engineFuel": {
          "$ref": "#/definitions/engineFuel"
        },
        "EnginName": {
          "$ref": "#/definitions/stringModel"
        },
        "EnginVendor": {
          "$ref": "#/definitions/stringModel"
        },
        "EnginePower": {
          "$ref": "#/definitions/stringModel"
        },
        "EnginStatus": {
          "$ref": "#/definitions/usageStatus"
        }
      }
    }
  }
}